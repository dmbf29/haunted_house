<p id="notice"><%= notice %></p>

<h1>Spooky Houses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Banner url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spooky_houses.each do |spooky_house| %>
      <tr>
        <td><%= spooky_house.name %></td>
        <td><%= spooky_house.banner_url %></td>
        <td><%= link_to 'Show', spooky_house %></td>
        <td><%= link_to 'Edit', edit_spooky_house_path(spooky_house) %></td>
        <td><%= link_to 'Destroy', spooky_house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spooky House', new_spooky_house_path %>
